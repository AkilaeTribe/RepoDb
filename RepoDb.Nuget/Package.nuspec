    <tags>orm sql repo repodb repository</tags>
  </metadata>
  <files>
    <file src="readme.txt" target="lib\RepoDb\readme.txt" />
    <file src="RepoDb.dll" target="lib\RepoDb\RepoDb.dll" />
  </files>
</package>